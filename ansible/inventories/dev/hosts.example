# ==================================
# Development Inventory Example
# ==================================
# Copy this file to 'hosts' and update with your actual EC2 instance IPs

[web_servers]
# Add your development web server IPs here
# dev-web01 ansible_host=10.0.11.10 ansible_user=ec2-user

[all:vars]
ansible_user=ec2-user
ansible_ssh_private_key_file=~/.ssh/self-healing-infra-key.pem
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
environment=dev
app_name=self-healing-app-dev
asg_name=self-healing-infra-dev-asg
